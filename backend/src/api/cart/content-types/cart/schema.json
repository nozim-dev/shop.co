{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "singularName": "cart",
    "pluralName": "carts",
    "displayName": "cart"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "required": true
    },
    "productUrl": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "costProduct": {
      "type": "integer",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    }
  }
}
